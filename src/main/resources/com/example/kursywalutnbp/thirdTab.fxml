        <yAxis>
          <NumberAxis fx:id="yAxis" side="LEFT" />
        </yAxis>
      </LineChart>
      <DatePicker fx:id="datePicker" layoutX="60.0" layoutY="360.0" />
      <ChoiceBox fx:id="choiceBox" layoutX="246.0" layoutY="360.0" prefHeight="26.0" prefWidth="230.0" />
      <Button fx:id="buttonDownload" layoutX="488.0" layoutY="360.0" mnemonicParsing="false" onAction="#getXMLButton" text="Pobierz dane" />
